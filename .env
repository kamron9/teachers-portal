# Development Environment Configuration
NODE_ENV=development

# Backend API Configuration
VITE_API_BASE_URL=http://localhost:3001/api/v1
VITE_SOCKET_URL=http://localhost:3001

# Frontend Configuration
VITE_APP_NAME=TutorUZ
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_REAL_PAYMENTS=false
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SOCKET_IO=true

# Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp,application/pdf

# Currency Configuration
VITE_DEFAULT_CURRENCY=UZS
VITE_TIMEZONE=Asia/Tashkent
